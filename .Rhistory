library(dplyr)
ggtexttable(table_laboat, theme = ttheme("light"), rows = NULL) %>% tab_add_title(text = "D", face = "bold", padding = unit(0.1, "line"))
table_figure <-ggtexttable(table_laboat, theme = ttheme("light"), rows = NULL) %>% tab_add_title(text = "D", face = "bold", padding = unit(0.1, "line"))
#aggrange everything
